<form method="post" enctype="multipart/form-data" action="@Url.Action("Save", "Home")">
    <button id="fillform">Preenche form</button>
    @Html.Partial("Partials/Form/UploadDocuments")
    @Html.Partial("Partials/Form/PersonalData")
    @Html.Partial("Partials/Form/FinancialData")
    @Html.Partial("Partials/Form/RevenueData")
    <div class="form-group">
        <input type="submit" value="Enviar Informações" class="btn btn-primary" />
    </div>
</form>


<div class="modal fade" id="modal-output" data-backdrop="static">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Processando Imagem. Aguarde...</h4>
            </div>
            <div class="modal-body">
                <div class="text-xs-center" id="progress"></div>
                <progress class="progress" value="0" max="1" aria-describedby="progress">
                    <div class="progress">
                        <span class="progress-bar" style="width: 25%;"></span>
                    </div>
                </progress>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Fechar</button>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
<!-- /.modal -->
