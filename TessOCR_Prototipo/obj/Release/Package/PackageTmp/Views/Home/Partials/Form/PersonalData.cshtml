<div class="card card-block">
    <h5 class="card-title">Dados Pessoais</h5>
    <hr />

    <div class="form-group">
        <label for="name">Nome</label>
        <input type="text" value="" placeholder="Nome completo do cliente" class="form-control" id="name" />
    </div>

    <div class="form-group">
        <label for="rg">RG</label>
        <input type="text" value="" placeholder="Número de Registro" class="form-control" id="rg"/>
    </div>

    <div class="form-group">
        <label for="dtNasc">Data de Nascimento</label>
        <input type="text" value="" placeholder="Data de Nascimento" class="form-control" id="dtNasc"/>
    </div>

    <div class="form-group">
        <label for="emailInput">Email</label>
        <input type="text" value="" placeholder="Endereço de Email" class="form-control" id="emailInput"/>
    </div>

    <div class="form-group">
        <label for="civilState">Estado Civil</label>
        <select class="form-control" id="civilState">
            <option value="casado">Casado</option>
            <option value="solteiro">Solteiro</option>
        </select>
    </div>

    <div class="form-group">
        <label for="link">Vínculo</label>
        <select class="form-control" id="link">
            <option value="1">Vínculo 1</option>
            <option value="2">Vínculo 2</option>
        </select>
    </div>

    <div class="form-group">
        <label for="profession">Profissão</label>
        <select class="form-control" id="profession">
            <option value="Desenvolvedor">Desenvolvedor</option>
            <option value="Carpinteiro">Carpinteiro</option>
            <option value="Outra">Outra</option>
        </select>
    </div>

    <div class="form-group">
        <label for="company">Empresa</label>
        <input type="text" value="" placeholder="Empresa" class="form-control" id="company" />
    </div>

    <div class="form-check">
        <label class="form-check-label">
            <input type="checkbox" class="form-check-input" id="isUnemployed">
            Não estou trabalhando atualmente
        </label>
    </div>
</div>



